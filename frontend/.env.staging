# Staging Environment Configuration
VITE_APP_TITLE=Training Management System - Staging
VITE_API_BASE_URL=https://staging-api.health.gov.bw/api
VITE_APP_VERSION=1.0.0-staging
VITE_APP_ENVIRONMENT=staging

# Database Configuration
VITE_DB_TYPE=postgresql

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_ANALYTICS=true

# Authentication
VITE_AUTH_TOKEN_EXPIRY=7200
VITE_AUTH_REFRESH_THRESHOLD=600

# UI Configuration
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_FILE_SIZE=10485760
VITE_SUPPORTED_FILE_TYPES=pdf,doc,docx,xls,xlsx,jpg,png

# Botswana Specific
VITE_COUNTRY_CODE=BW
VITE_CURRENCY=BWP
VITE_TIMEZONE=Africa/Gaborone
VITE_LANGUAGE=en
